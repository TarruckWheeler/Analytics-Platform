    <!-- ===== SIDEBAR ===== -->
    <nav class="sidebar" id="sidebar">
        <div class="nav-section">
            <h3>Analytics¬†Tools</h3>
            <div class="nav-item active" onclick="NexusApp.switchWorkspace('network',event)"><div class="nav-icon">üï∏Ô∏è</div><span>Network¬†Analysis</span></div>
            <div class="nav-item"        onclick="NexusApp.switchWorkspace('geospatial',event)"><div class="nav-icon">üó∫Ô∏è</div><span>Geospatial¬†Analysis</span></div>
            <div class="nav-item"        onclick="NexusApp.switchWorkspace('timeline',event)"><div class="nav-icon">üìä</div><span>Timeline¬†Analytics</span></div>
